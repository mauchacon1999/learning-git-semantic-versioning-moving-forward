{
  "name": "learning-git-semantic-versioning",
  "version": "1.0.0",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "husky": "^9.1.7",
    "semantic-release": "^24.2.7"
  },
  "scripts": {
    "prepare": "husky",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "release:debug": "DEBUG=semantic-release:* semantic-release",
    "release:minor": "semantic-release --release-as minor",
    "release:1.1.0": "semantic-release --release-as 1.1.0",
    "release:1.2.0": "semantic-release --release-as 1.2.0",
    "release:custom": "semantic-release --release-as",
    "release:env": "semantic-release --release-as $env:RELEASE_VERSION"
  }
}
